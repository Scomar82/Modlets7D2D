			<property name="CustomIcon" value="modGunHeadShot"/>
			<property name="Unlocks" value="modGunHeadShot"/>
			<effect_group tiered="false">
				<triggered_effect trigger="onSelfPrimaryActionEnd" action="ModifyCVar" cvar="modGunHeadShot" operation="set" value="1"/>
