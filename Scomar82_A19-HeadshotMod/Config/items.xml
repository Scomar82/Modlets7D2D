<Scomar82_HeadshotMod>
	
	<append xpath="/items" >

		<item name="emptybookzeds">
			<property name="Extends" value="questMaster" />
			<property name="CustomIcon" value="emptybookzeds"/>
			<property name="Description" value="emptybookzedsDesc"/>
			<property name="Group" value="Science"/>
			<property class="Action0">
				<property name="QuestGiven" value="q_emptybookzeds" />
			</property>
		</item>
		
